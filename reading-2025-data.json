---
layout: null
---
[
  {% for book in site.data.reading-2025 %}
    {
      "date": {{ book.Date | jsonify }},
      "title": {{ book.Title | jsonify }},
      "category": {{ book.Category | jsonify }},
      "subcategory": {% if book.Category == "Politics" %}{{ book.Politics | jsonify }}{% elsif book.Category == "Technology" %}{{ book.Technology | jsonify }}{% elsif book.Category == "Religion" %}{{ book.Religion | jsonify }}{% else %}null{% endif %},
      "type": {{ book.Type | jsonify }},
      "rating": {{ book.Rating | jsonify }},
      "format": {{ book.Format | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
